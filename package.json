{
  "name": "@bitblit/epsilon",
  "version": "0.0.0",
  "description": "Tiny adapter to simplify building API gateway Lambda APIS",
  "main": "./dist/index",
  "typings": "./dist/index",
  "directories": [
    "dist"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepare-to-publish": "yarn clean-compile && yarn run apply-circle && yarn remove-compiled-tests",
    "copy-static-files": "shx cp -R src/static dist",
    "clean-compile": "yarn run clean && yarn run generate-barrels && yarn run compile",
    "compile": "tsc && yarn copy-static-files",
    "watch": "tsc-watch",
    "remove-compiled-tests": "rimraf ./lib/**/*.spec.d.ts && rimraf ./lib/**/*.spec.js && rimraf ./lib/**/*.spec.js.map",
    "clean": "shx rm -Rf dist",
    "compile-test": "yarn compile && yarn test",
    "test": "jest",
    "apply-circle": "node node_modules/@bitblit/ratchet/dist/node-only/circleci/apply-circle-ci-env-variables-to-files dist/static/build-properties.json",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "generate-barrels": "barrelsby --delete -d src -e .*\\.spec\\.ts -e .*sample.*",
    "run-sample-server": "yarn compile && node dist/sample/sample-local-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Epsilon"
  },
  "keywords": [
    "epsilon",
    "lambda",
    "api gateway"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Epsilon/issues"
  },
  "homepage": "https://github.com/bitblit/Epsilon#readme",
  "engines": {
    "node": ">=14.15"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.16.1026",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.3",
    "luxon": "2.0.1",
    "cross-fetch": "3.1.4",
    "route-parser": "0.0.5",
    "strip-js": "1.2.0",
    "swagger-model-validator": "3.0.20",
    "rxjs": "7.2.0"
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "minimist": "1.2.5",
    "object-path": "^0.11.5",
    "node-fetch": "^2.6.1",
    "axios": "^0.21.1"
  },
  "peerDependencies": {
    "@types/aws-lambda": "8.10.81",
    "aws-sdk": "2.960.0"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "1.208.0",
    "@types/aws-lambda": "8.10.81",
    "@types/js-yaml": "4.0.2",
    "@types/luxon": "1.27.1",
    "@types/node": "14.14.31",
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "apollo-server-lambda": "2.21.0",
    "graphql": "15.5.0",
    "aws-sdk": "2.960.0",
    "barrelsby": "2.2.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "graphql": "15.4.0",
    "handlebars": "4.7.7",
    "handlebars-layouts": "3.1.4",
    "husky": "4.3.8",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "shx": "0.3.3",
    "ts-jest": "27.0.3",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
}
