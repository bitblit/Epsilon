{
  "name": "@bitblit/epsilon",
  "version": "0.7.0",
  "description": "Tiny adapter to simplify building API gateway Lambda APIS",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "compile": "yarn run clean && tsc && yarn run copy-static-files",
    "watch": "tsc -w .",
    "clean": "shx rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "copy-static-files": "cd src && shx cp -R static ../dist",
    "apply-circle": "node node_modules/@bitblit/ratchet/dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json",
    "run-sample-server": "yarn compile && node dist/sample-local-server.js",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Epsilon"
  },
  "keywords": [
    "epsilon",
    "lambda",
    "api gateway"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Epsilon/issues"
  },
  "homepage": "https://github.com/bitblit/Epsilon#readme",
  "engines": {
    "node": ">=12.8"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.10.2",
    "@bitblit/saltmine": "0.5.4",
    "js-yaml": "4.0.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.12.2",
    "moment": "2.29.1",
    "moment-timezone": "0.5.32",
    "portable-fetch": "3.0.0",
    "route-parser": "0.0.5",
    "strip-js": "1.2.0",
    "swagger-model-validator": "3.0.20"
  },
  "resolutions": {
    "lodash": "^4.17.20",
    "minimist": "1.2.5",
    "object-path": "^0.11.5",
    "node-fetch": "^2.6.1",
    "axios": "^0.21.1"
  },
  "peerDependencies": {
    "@types/aws-lambda": "8.10.70",
    "aws-sdk": "2.824.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.70",
    "@types/chai": "4.2.14",
    "@types/js-yaml": "4.0.0",
    "@types/mocha": "8.2.0",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "apollo-server-lambda": "2.19.1",
    "aws-sdk": "2.824.0",
    "chai": "4.2.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "graphql": "15.4.0",
    "husky": "5.0.6",
    "mocha": "8.2.1",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "shx": "0.3.3",
    "ts-node": "9.1.1",
    "typescript": "3.9.7"
  }
}
