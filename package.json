{
  "name": "@bitblit/epsilon",
  "version": "0.0.0",
  "description": "Tiny adapter to simplify building API gateway Lambda APIS",
  "sideEffects": false,
  "bin": {
    "epsilon-run-background-process-from-command-line-dry-run": "./cli-bootstrap/epsilon-run-background-process-from-command-line-dry-run.js"
  },
  "main": "./index.js",
  "typings": "./index.d.ts",
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>",
    "Lucas Myers <veganbeef@protonmail.com>",
    "Zach Herridge"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepare-to-publish": "yarn build && cp package.json README.md CHANGELOG.md License.txt dist && yarn run apply-ci-vars && yarn remove-compiled-tests",
    "copy-static-files": "shx cp -R src/static dist",
    "build": "yarn run clean && yarn run generate-barrels && yarn run compile",
    "compile": "tsc && yarn copy-static-files",
    "watch": "tsc-watch",
    "remove-compiled-tests": "rimraf ./lib/**/*.spec.d.ts && rimraf ./lib/**/*.spec.js && rimraf ./lib/**/*.spec.js.map",
    "clean": "shx rm -Rf dist",
    "compile-test": "yarn compile && yarn test",
    "test": "jest",
    "apply-ci-vars": "yarn ratchet-apply-ci-env-variables-to-files github dist/static/build-properties.json",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "generate-barrels": "barrelsby -q --delete -d src -e .*\\.spec\\.ts -e .*sample.* -e deployment/.* && barrelsby -q --delete -d src/deployment -e .*\\.spec\\.ts ",
    "run-local-container-server": "node dist/local-container-server.js",
    "run-sample-server": "yarn compile && node dist/sample/sample-local-server.js",
    "run-test-error-server": "yarn compile && node dist/test-error-server.js",
    "run-batch-sample-server": "yarn compile && node dist/sample/sample-batch-only-local-server.js",
    "run-background-process-from-command-line-dry-run": "yarn compile && node dist/cli-bootstrap/epsilon-run-background-process-from-command-line-dry-run.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Epsilon"
  },
  "keywords": [
    "epsilon",
    "lambda",
    "api gateway"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Epsilon/issues"
  },
  "homepage": "https://github.com/bitblit/Epsilon#readme",
  "engines": {
    "node": ">=14.18"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "3.2.71",
    "clear": "0.1.0",
    "commander": "10.0.0",
    "cross-fetch": "3.1.5",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.0",
    "jwks-rsa": "3.0.1",
    "luxon": "3.2.1",
    "reflect-metadata": "0.1.13",
    "route-parser": "0.0.5",
    "rxjs": "7.8.0",
    "strip-js": "1.2.0",
    "swagger-model-validator": "3.0.21"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "ajv": "^8.12.0",
    "axios": "^1.2.3",
    "browserslist": "^4.21.4",
    "express": "^4.18.2",
    "follow-redirects": "^1.15.2",
    "glob-parent": "^6.0.2",
    "hosted-git-info": "^6.1.1",
    "jose": "^4.11.2",
    "json5": "^1.0.2",
    "lodash": "^4.17.21",
    "minimist": "^1.2.7",
    "nanoid": "^4.0.0",
    "node-fetch": "^2.6.7",
    "nth-check": "^2.1.1",
    "object-path": "^0.11.8",
    "path-parse": "^1.0.7",
    "shelljs": "^0.8.5",
    "sanitize-html": "^2.8.0",
    "semver-regex": "^4.0.5",
    "tmpl": "^1.0.5",
    "ws": "^8.12.0",
    "y18n": "^5.0.8"
  },
  "peerDependencies": {
    "@bitblit/ratchet": "3.2.71",
    "@types/aws-lambda": "8.10.110",
    "aws-cdk-lib": "2.65.0",
    "aws-sdk": "2.1316.0",
    "constructs": "10.1.252",
    "graphql": "16.6.0"
  },
  "peerDependenciesMeta": {
    "aws-cdk-lib": {
      "optional": true
    },
    "constructs": {
      "optional": true
    },
    "graphql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.110",
    "@types/jest": "29.4.0",
    "@types/js-yaml": "4.0.5",
    "@types/luxon": "3.2.0",
    "@types/node": "16.11.58",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "apollo-server-lambda": "3.11.1",
    "aws-cdk-lib": "2.65.0",
    "aws-sdk": "2.1316.0",
    "barrelsby": "2.5.1",
    "constructs": "10.1.252",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "graphql": "16.6.0",
    "handlebars": "4.7.7",
    "handlebars-layouts": "3.1.4",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "pretty-quick": "3.1.3",
    "rimraf": "4.1.2",
    "shx": "0.3.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
