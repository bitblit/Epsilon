{
  "name": "@bitblit/epsilon",
  "version": "0.1.21",
  "description": "Tiny adapter to simplify building API gateway Lambda APIS",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>"
  ],
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "compile": "yarn run clean && tsc && yarn run copy-static-files",
    "watch": "tsc -w .",
    "clean": "rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "copy-static-files": "cd src && cp -R static ../dist",
    "apply-circle": "node node_modules/@bitblit/ratchet/dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json",
    "run-sample-server": "yarn compile && node dist/sample-local-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Epsilon"
  },
  "keywords": [
    "epsilon",
    "lambda",
    "api gateway"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Epsilon/issues"
  },
  "homepage": "https://github.com/bitblit/Epsilon#readme",
  "engines": {
    "node": ">=8.9.4"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.5.7",
    "@bitblit/saltmine": "0.3.3",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.5.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.26",
    "node-yaml": "3.2.0",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "route-parser": "0.0.5",
    "strip-js": "1.2.0",
    "swagger-model-validator": "3.0.5"
  },
  "resolutions": {
    "lodash": "4.17.15"
  },
  "peerDependencies": {
    "@types/aws-lambda": "8.10.33",
    "aws-sdk": "2.552.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.33",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "@types/node": "6.0.46",
    "apollo-server-lambda": "2.8.1",
    "aws-sdk": "2.552.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "graphql": "14.4.2",
    "mocha": "6.2.0",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  }
}
