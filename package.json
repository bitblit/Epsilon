{
  "name": "@bitblit/epsilon",
  "version": "0.0.0",
  "description": "Tiny adapter to simplify building API gateway Lambda APIS",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>"
  ],
  "config": {},
  "scripts": {
    "compile": "yarn run clean && tsc && yarn run copy-static-files",
    "copy-static-files": "cd src && cp -R static ../dist",
    "watch": "tsc -w .",
    "clean": "rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "run-sample-server": "yarn compile && node dist/sample-local-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Epsilon"
  },
  "keywords": [
    "epsilon",
    "lambda",
    "api gateway"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Epsilon/issues"
  },
  "homepage": "https://github.com/bitblit/Epsilon#readme",
  "engines": {
    "node": ">=8.9.4"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet": "0.0.95",
    "@bitblit/saltmine": "0.2.2",
    "crypt3": "^1.0.0",
    "jsonwebtoken": "^8.1.1",
    "node-yaml": "^3.1.1",
    "route-parser": "^0.0.5",
    "strip-js": "^1.2.0",
    "swagger-model-validator": "^2.2.8",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14"
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.31",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^6.0.46",
    "aws-lambda-typescript": "^1.0.2",
    "aws-sdk": "^2.195.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.5.3",
    "ts-node": "^3.3.0",
    "tslint": "~5.7.0",
    "typescript": "^2.7.1"
  }
}
